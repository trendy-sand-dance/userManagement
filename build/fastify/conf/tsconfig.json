{
	"compilerOptions": {
	  "target": "ESNext",
	  "module": "NodeNext",
	  "moduleResolution": "NodeNext",
	  "outDir": "./dist",
	  "rootDir": "./fastify",
	  "strict": true,
	  "esModuleInterop": true,
	  "skipLibCheck": true,
	  "forceConsistentCasingInFileNames": true
	},
	"include": ["fastify"],
	"exclude": ["node_modules", "**/*.spec.ts"]
}